<odoo noupdate="1">
  <!-- Project task stages -->
  <record id="project_stage_todo" model="project.task.type">
    <field name="name">To Do</field>
    <field name="sequence">1</field>
  </record>
  <record id="project_stage_progress" model="project.task.type">
    <field name="name">In Progress</field>
    <field name="sequence">2</field>
  </record>
  <record id="project_stage_done" model="project.task.type">
    <field name="name">Done</field>
    <field name="sequence">3</field>
  </record>
</odoo>